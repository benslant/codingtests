generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model group {
  id   String  @id
  name String? @unique @db.VarChar(50)
}

model user {
  id         String  @id
  first_name String? @db.VarChar(50)
  last_name  String? @db.VarChar(50)
  email      String? @unique @db.VarChar(50)
}
